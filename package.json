{
  "name": "ngx-dynamic-form-builder",
  "version": "1.0.0",
  "description": "FormBuilder + class-transformer + class-validator = dynamic form group builder for Angular7+",
  "keywords": [
    "form-builder",
    "class-transformer",
    "class-validator",
    "auto",
    "dynamic",
    "formgroup",
    "form",
    "validator",
    "validation",
    "object",
    "class",
    "transformer",
    "transform",
    "builder",
    "formarray",
    "angular",
    "angular7"
  ],
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=10",
    "npm": ">=6.4.0"
  },
  "bugs": {
    "url": "https://github.com/Metacarp/ngx-dynamic-form-builder.git"
  },
  "homepage": "https://github.com/Metacarp/ngx-dynamic-form-builder.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Metacarp/ngx-dynamic-form-builder.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "config:mono": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:mono": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-lib": "ng build ngx-dynamic-form-builder --prod",
    "lib:build": "run-s prepare:prod lib:build-lib",
    "app:prebuild": "ng build demo --prod",
    "app:build": "run-s format:fix prepare:mono app:prebuild",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve",
    "start": "run-s format:fix prepare:mono serve",
    "build": "run-s app:build",
    "test": "run-s github:build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "github:prebuild": "ng build demo --base-href /ngx-dynamic-form-builder/ --prod",
    "github:build": "run-s format:fix lib:build config:prod github:prebuild github:postbuild",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/demo/index.html ./dist/demo/404.html",
    "github:deploy": "gh-pages -d dist/demo",
    "format:fix": "prettier  --config ./.prettierrc --write \"{apps,libs}/**/*.ts\""
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/pwa": "~0.803.20",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@ngx-prism/core": "^2.0.1",
    "@types/prismjs": "^1.16.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "core-js": "^3.4.5",
    "lodash-es": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "ngx-markdown": "^8.2.1",
    "prismjs": "^1.17.1",
    "rxjs": "^6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular-devkit/build-ng-packagr": "~0.803.20",
    "@angular/cli": "^8.3.20",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@rucken/cli": "^4.1.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.mergewith": "^4.6.6",
    "@types/node": "~12.12.14",
    "codelyzer": "^5.0.1",
    "conventional-changelog-cli": "^2.0.28",
    "conventional-commits-detector": "^1.0.2",
    "conventional-recommended-bump": "^6.0.5",
    "cp-cli": "^2.0.0",
    "gh-pages": "^2.1.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lite-server": "^2.5.4",
    "markdown-loader": "^5.1.0",
    "ng-packagr": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.5.4",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  }
}
